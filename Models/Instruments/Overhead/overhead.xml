<?xml version="1.0" encoding="UTF-8" ?>

<!-- Airbus A340 for FlightGear -->
<!-- Mode Control Panel -->

<PropertyList>


	<path>../../../Models/Instruments/Overhead/overhead.ac</path>

 <animation>
  <type>scale</type>
  <x-offset>0.75</x-offset>
  <y-offset>0.85</y-offset>
  <z-offset>0.75</z-offset>
 </animation>


<animation>
  <type>material</type>
    <object-name>overhead</object-name>
  <emission>
    <red>0.3</red>
    <green>0.3</green>
    <blue>0.3</blue>
	<factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
</animation>

<animation>
  <type>material</type>
	<object-name>dome</object-name>
	<object-name>landing</object-name>
	<object-name>nav</object-name>
	<object-name>strobe</object-name>
	<object-name>beacon</object-name>
	<object-name>seatbelts</object-name>
  <emission>
    <red>0.3</red>
    <green>0.3</green>
    <blue>0.3</blue>
	<!--factor-prop>controls/lighting/instruments-norm</factor-prop-->
  </emission>
</animation>


<!--Boton de la APU BAT -->

<animation>
  <type>select</type>
    <object-name>botona.002</object-name>
	<condition>
	<property>/controls/APU/apu-gen-switch</property>
	<value>1</value>
	</condition>
</animation>


 <animation>
  <type>pick</type>
   <object-name>botonb.002</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
		<command>property-toggle</command>
      <property>/controls/APU/apu-gen-switch</property>
		<!--value>1</value-->
    </binding>
	<binding>
                <command>nasal</command>
                <script>controls.click(1)</script>
            </binding>
   </action>
 </animation>

 <animation>
  <type>pick</type>
   <object-name>botona.002</object-name>
   <action>
