	<!-- Panel lighting -->
	<animation>
		<name>Panel lighting select</name>
		<type>select</type>
		<object-name>MarkingLts</object-name>
		<condition>
			<or>
				<not>
					<property>controls/lighting/dome-norm</property>
				</not>
				<and>
					<equals>
						<property>controls/lighting/dome-norm</property>
						<value>0.5</value>
					</equals>
					<greater-than>
						<property>controls/lighting/overhead-panel-norm</property>
						<value>0.25</value>
					</greater-than>
				</and>
				<and>
					<equals>
						<property>controls/lighting/dome-norm</property>
						<value>1</value>
					</equals>
					<greater-than>
						<property>controls/lighting/overhead-panel-norm</property>
						<value>0.5</value>
					</greater-than>
				</and>
			</or>
		</condition>
	</animation>
