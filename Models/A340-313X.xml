<!-- Fuel Truck -->

<model>
	<name>fuel-truck</name>
	<path>Aircraft/A340-313X/Models/Operations/Fuel-Truck/Fuel_Truck_Short_VolvoFM.xml</path>
	<offsets>
		<x-m>-9</x-m>
     	 	<y-m>3</y-m>
     		 <z-m>-0.84</z-m>
	</offsets>
	<condition>
		<and>
		<less-than>	
	         <property>/velocities/groundspeed-kt</property>
	         <value>1</value>
	         </less-than>
		<equals>
			<property>/services/fuel-truck/enable</property>
			<value>1</value>
		</equals>
		</and>
	</condition>
</model>


