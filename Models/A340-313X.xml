<PropertyList>


    <model>
      <path>Aircraft/A340-313X/Models/A340-313X-avion.xml</path>
       <offsets>
                <x-m>0</x-m>
                <y-m>0</y-m>
                <z-m>-0.35</z-m>
          <pitch-deg>-0.8</pitch-deg>
       </offsets>
                <livery>
                    <file type="string">House</file>
                </livery>
    </model>

	<nasal>
		<load>
		     # livery path
  	 	     var livery_update = aircraft.livery_update.new("Aircraft/A340-313X/Models/Liveries")
		</load>

		<unload>
			livery_update.stop();
		</unload>
	</nasal>


<!-- Shadow -->

  <model>
    <name>shadowOSG</name>
    <path>Aircraft/A340-313X/Models/shadow.xml</path>
 <offsets>
   <x-m> 0.0</x-m>
   <y-m> 0.0</y-m>
   <z-m> 1.0</z-m>
   <pitch-deg>0</pitch-deg>
 </offsets>
  </model>

  <animation>
    <type>select</type>
    <object-name>shadowOSG</object-name>
    <condition>
      <equals>
        <property>sim[0]/rendering/shadows-ac</property>
        <value>true</value>
      </equals>
    </condition>
  </animation>

<!-- Airport Operations -->

 <model>
 <name>Followme</name>
 <path>Aircraft/A340-313X/Models/Operations/Followme/followme.xml</path>
 <condition>
 <equals>
			             <property>controls/switches/followme</property>
                                            <value type="bool">true</value>
 </equals>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>80</value>
            </less-than>
  </condition>   
  <offsets>
  <x-m>-50</x-m>
            <y-m>0</y-m>
            <z-m>-5.25</z-m>
 </offsets>
</model>

 <model>
 <name>Catering Vehicle</name>
 <path>Aircraft/A340-313X/Models/Operations/Catering/catering.xml</path>
 <condition>
 <equals>
			             <property>controls/switches/catering</property>
                                            <value type="bool">true</value>
 </equals>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>15</value>
            </less-than>
  </condition>   
  <offsets>
  <x-m>16</x-m>
  <y-m> 10</y-m>
  <z-m>-5.25</z-m>
<heading-deg>180</heading-deg>
 </offsets>
</model>

