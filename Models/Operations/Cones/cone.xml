<?xml version="1.0"?>

<PropertyList>

 <path>Cone.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>2000</max-m>
 </animation>

  <animation>
    <type>material</type>
    <object-name>Base</object-name>
    <object-name>Cone</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission>
      <red>0.9</red>
      <green>0.9</green>
      <blue>0.9</blue>
    </emission>
  </animation>

</PropertyList>
